@model Deneme.Models.Şablon_CariBilgileri
@{
    /**/

    /**/

    Layout = null;
}

@using (Html.BeginForm("AddOrEdit_CBS", "CurrentInformationsTemplate", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.ŞablonId)
<div class="block push-bit">

    <hr />
