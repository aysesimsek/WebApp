                <label class="control-label " for="CurrentCode">@Deneme.Resource.CurrentCode * </label>
                @Html.DropDownList("CariKodu", new SelectList(Enum.GetValues(typeof(Deneme.Models.Şablon_CariBilgileri.Alphabet))), Deneme.Resource.Select + ' ' + Deneme.Resource.CurrentCode + ' ' + Deneme.Resource.Select2, new { @class = "required form-control" })
                @Html.ValidationMessageFor(model => model.CariKodu, "", new { @class = "text-danger" })

